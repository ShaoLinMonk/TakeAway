<states>
	<state>
		<name>OrderSpecified</name>
		<api-list>
			<api>
				<url>setAddressById</url>
				<condition>
					<statement>isLogin</statement>
					<statement>isValidAddressId</statement>
				</condition>
				<end-state>
					<true>AddressSpecified</true>
					<false>OrderSpecified</false>
				</end-state>
			</api>
			<api>
				<url>setAddress</url>
				<condition>
					<statement>isLogin</statement>
					<statement>isValidAddress</statement>
				</condition>
				<end-state>
					<true>AddressSpecified</true>
					<false>OrderSpecified</false>
				</end-state>
			</api>
			<api>
				<url>createNewUser</url>
				<condition>
					<or>
						<and>
							<not>
								<statement>isLogin</statement>
							</not>
							<statement>isValidAddress</statement>
						</and>
						<statement>isLogin</statement>
					</or>
				</condition>
				<end-state>
					<true>AddressSpecified</true>
					<false>OrderSpecified</false>
				</end-state>
			</api>
		</api-list>
	</state>
</states>